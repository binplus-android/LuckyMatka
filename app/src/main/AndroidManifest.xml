        <activity
            android:name="in.games.luckymatkagames.Activity.AllChartActivity"
            android:theme="@style/Theme.JollyMatka.Material2" />
        <activity
            android:name="in.games.luckymatkagames.Activity.ChartWebViewActivity"
            android:theme="@style/Theme.JollyMatka.Material2" />
        <activity
            android:name="in.games.luckymatkagames.Activity.FullSangamActivity"
            android:theme="@style/Theme.JollyMatka.Material2" />
        <activity
            android:name="in.games.luckymatkagames.Activity.HalfSangamActivity"
