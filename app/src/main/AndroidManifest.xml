        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JollyMatka.Material"
        android:usesCleartextTraffic="true">

        <activity
            android:name="in.games.luckymatkagames.Activity.SpMotorActivity"
            android:theme="@style/Theme.JollyMatka.Material2" />
        <activity android:name="in.games.luckymatkagames.Activity.StarGameListActivity" />
        <activity android:name="in.games.luckymatkagames.Activity.NoticeBoardActivity" />
        <activity android:name="in.games.luckymatkagames.Activity.NotifiactionActivity" />
        <activity android:name="in.games.luckymatkagames.Activity.SelectChartActivity" />
        <activity android:name="in.games.luckymatkagames.Activity.MatkanameHistory" />
        <activity android:name="in.games.luckymatkagames.Activity.ChartActivity" />
        <activity android:name="in.games.luckymatkagames.Activity.StarlineActivity" />
        <activity android:name="in.games.luckymatkagames.Activity.StarWinHistory" />
        <activity android:name="in.games.luckymatkagames.Activity.BankDetailActivity" />
        <activity android:name="in.games.luckymatkagames.Activity.RequestFundsActivity" />
